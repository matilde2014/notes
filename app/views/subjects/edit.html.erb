<h1>Editing subject</h1>

<%= render 'form' %>

<table class="table table-striped table-hover">
  <thead>
  <tr>
    <th>Name</th>
    <th>Birthday</th>
    <th>nif</th>
    <th></th>
  </tr>
  </thead>

  <tbody>
  <% @students.each do |student| %>
      <tr>
        <td><%= link_to student.name, student  %></td>
        <td><%= student.birthday %></td>
        <td><%= student.nif %></td>
        <td><%= render 'enroll_form', student: student %></td>
        </tr>
  <% end %>
  </tbody>
</table>

<%= link_to 'Show', @subject %> |
<%= link_to 'Back', subjects_path %>


